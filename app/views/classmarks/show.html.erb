<div id="content_detail" class="ui-corner-all">
<h1 class="title"><%= t('page.showing', :model => t('activerecord.models.classmark')) -%></h1>
<div id="content_list">
<p id="notice"><%= notice %></p>

<p>
  <strong><%= t('activerecord.attributes.classmark.name') -%>:</strong>
  <%= @classmark.name -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.classmark.display_name') -%>:</strong>
  <%= @classmark.display_name -%>
</p>

</div>
</div>

<div id="submenu" class="ui-corner-all">
  <ul>
    <li><%= link_to t('page.edit'), edit_classmark_path(@classmark) -%></li>
    <li><%= link_to t('page.back'), classmarks_path -%></li>
  </ul>
</div>

<div id="content_detail" class="ui-corner-all">
<h1 class="title"><%= t('page.showing', :model => t('activerecord.models.user_status')) -%></h1>
<div id="content_list">
<p id="notice"><%= notice %></p>

<p>
  <strong><%= t('activerecord.attributes.user_status.name') -%>:</strong>
  <%= @user_status.name -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.user_status.display_name') -%>:</strong>
  <%= @user_status.display_name -%>
</p>

</div>
</div>

<div id="submenu" class="ui-corner-all">
  <ul>
    <li><%= link_to t('page.edit'), edit_user_status_path(@user_status) -%></li>
    <li><%= link_to t('page.back'), user_statuses_path -%></li>
  </ul>
</div>

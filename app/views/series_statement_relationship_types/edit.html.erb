<div id="content_detail" class="ui-corner-all">
  <h1 class="title"><%= t('page.editing', :model => t('activerecord.models.series_statement_relationship_type')) %></h1>
  <div id="content_list">

    <%= form_for(@series_statement_relationship_type) do |f| %>
      <%= f.error_messages %>
      <div class="field">
        <%= f.label :display_name %><br />
        <%= f.text_field :display_name %>
      </div>
      <div class="field">
        <%= f.label :typeid %><br />
        <%= f.text_field :typeid %>
      </div>
      <div class="field">
        <%= f.label :note %><br />
        <%= f.text_area :note, :class => 'resource_textarea' %>
      </div>
      <div class="actions">
        <%= f.submit %>
      </div>
    <% end %>
  </div>
</div>

<div id="submenu" class="ui-corner-all">
  <ul>
    <li><%= link_to t('page.show'), @series_statement_relationship_type %></li>
    <li><%= link_to t('page.back'), series_statement_relationship_types_path %></li>
  </ul>
</div>

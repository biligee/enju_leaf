<div id="content_detail" class="ui-corner-all">
  <h1 class="title"><%= t('page.listing', :model => t('activerecord.models.series_statement_relationship_type')) %></h1>
  <div id="content_list">
  <div style="color: green"><%= flash[:message] %></div>

    <table class="index">
      <tr>
        <th id="position"></th>
        <th><%= t('activerecord.attributes.series_statement_relationship_type.display_name') %></th>
        <th><%= t('activerecord.attributes.series_statement_relationship_type.typeid') %></th>
        <th><%= t('activerecord.attributes.series_statement_relationship_type.note') %></th>
        <th></th>
